{
    "custom/gpuinfo": {
        "exec": " gpuinfo.sh",
        "return-type": "json",
        "format": "{0}",
        "rotate": 0,
        "interval": 5,
        "tooltip": true,
        "max-length": 1000,
        "on-click": "gpuinfo.sh --toggle",
        "menu": "on-click-right",
        "menu-file": "~/.config/waybar/menus/gpuinfo.xml",
        "menu-actions": {
            "toggle": "gpuinfo.sh --toggle",
            "reset": "gpuinfo.sh --reset"
        }
    }
}