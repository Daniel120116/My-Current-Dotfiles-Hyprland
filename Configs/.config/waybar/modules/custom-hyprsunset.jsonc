{
    "custom/hyprsunset": {
        "return-type": "json",
        "format": "{icon} ",
        "format-icons": {
            "active": "\uea70",
            "inactive": "\ueae7"
        },
        "exec": "hyprsunset.sh r q",
        "rotate": 0,
        "on-click": "hyprsunset.sh t",
        "on-scroll-up": "hyprsunset.sh i",
        "on-scroll-down": "hyprsunset.sh d",
        "interval": 1,
        "tooltip": true,
        "escape": true
    }
}