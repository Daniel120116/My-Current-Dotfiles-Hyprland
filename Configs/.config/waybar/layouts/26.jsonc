    "height": 10,
    "exclusive": true,
    "passthrough": false,
    "gtk-layer-shell": true,
    "reload_style_on_change": true,
    "include": [
        "/home/khing/.config/waybar/custom-modules.jsonc", // Your own custom configuration go here
