    "modules-left": [
        "group/pill#1"
    ],
    "modules-center": [
        "group/pill#2"
    ],
    "modules-right": [
        "group/pill#3"
    ],
    // positions generated based on config.ctl //
    "group/pill#1": [
        "hyprland/workspaces",
        "wlr/taskbar"
